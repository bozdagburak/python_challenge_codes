   "source": [
    "n = int(input(\"enter a positive number: \"))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range (1, n+1):\n",
    "    if n % i == 0:\n",
    "        count += 1\n",
    "if count == 2:\n",
    "    print(f\"{n} is a prime number.\")\n",
    "elif n == 0 or n == 1:\n",
    "    print(f\"{n} is not a prime number.\")\n",
    "else:\n",
    "    print(f\"{n} is not a prime number.\")"
   ]
  }
